<?xml version="1.0" encoding="UTF-8" ?>
<TabbedPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="MobileApp.NewMainPage">

    <ContentPage Title="HomePage" IconImageSource="outline_home_black_24dp.png">
        <ContentPage.Content>
            <StackLayout Padding="12, 12, 12, 12" VerticalOptions="EndAndExpand" HorizontalOptions="FillAndExpand">
                <Label Text="Welcome to HomePage" />
            </StackLayout>
        </ContentPage.Content>
    </ContentPage>

    <ContentPage Title="AccountPage" IconImageSource="outline_account_circle_black_24dp.png">
        <ContentPage.Content>
            <StackLayout Padding="12, 12, 12, 12" VerticalOptions="EndAndExpand" HorizontalOptions="FillAndExpand">
                <Label Text="Welcome to Account Page" />
            </StackLayout>
        </ContentPage.Content>
    </ContentPage>

</TabbedPage>
